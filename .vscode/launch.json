    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Lookup single host",
            "program": "${workspaceFolder}\\staging.js",
            "args": ["www.asos.com"],
            "env": {
                "debug" : "staging"
            }
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Lookup multiple hosts",
            "program": "${workspaceFolder}\\staging.js",
            "args": [
                "www.asos.com",
                "my.asos.com",
                "secure.asos.com"
            ],
            "env": {
                "debug" : "staging"
            }
        }
    ]
}